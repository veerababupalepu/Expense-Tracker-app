{
  "version": 2,
  "name": "expense-tracker",
  "builds": [
    { "src": "frontend/index.html", "use": "@vercel/static" },
    { "src": "frontend/assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/frontend/assets/$1", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/api/(.*)", "dest": "http://localhost:5000/api/$1" },
    { "src": "/(.*)", "dest": "/frontend/index.html" }
  ]
}


